{
  "name": "snyk-policy",
  "description": "Snyk's policy parser and matching logic",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc lib",
    "format": "prettier --write '{lib,test}/**/*.?s'",
    "check-tests": "! grep 'test.only' 'test/**/*.test.?s' -n",
    "env": "node -e 'console.log(process.env, process.versions)'",
    "tap": "tap  --node-arg=-r --node-arg=ts-node/register 'test/**/*.test.?s' -R spec --timeout=60",
    "test": "npm run check-tests && npm run lint && npm run tap",
    "prepare": "npm run build"
  },
  "keywords": [
    "snyk"
  ],
  "author": "Remy Sharp",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^6.14.9",
    "@types/semver": "^6.2.0",
    "eslint": "^4.0.0",
    "prettier": "^1.19.1",
    "proxyquire": "^2.1.0",
    "sinon": "^4.0.0",
    "tap": "^12.0.1",
    "tap-only": "0.0.5",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "email-validator": "^2.0.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "semver": "^6.0.0",
    "snyk-module": "^2.0.2",
    "snyk-resolve": "^1.0.1",
    "snyk-try-require": "^1.3.1",
    "then-fs": "^2.0.0",
    "tslib": "^1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/policy.git"
  }
}
